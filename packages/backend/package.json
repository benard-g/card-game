{
  "name": "@packages/backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node build/src/main.js",
    "build": "tsc",
    "build:clean": "rimraf build",
    "build:re": "yarn build:clean && yarn build",
    "dev": "tsc-watch --onSuccess 'yarn start'",
    "migrate": "typeorm migration:run",
    "test": "yarn test:lint && yarn test:migrate && yarn test:jest",
    "test:lint": "eslint --max-warnings=0 '{src,test}/**/*.{js,jsx,ts,tsx}'",
    "test:migrate": "NODE_ENV=test yarn migrate",
    "test:jest": "jest 'test/.*.test.ts' --runInBand --coverage"
  },
  "engines": {
    "node": "v14.16.0"
  },
  "dependencies": {
    "@packages/lib-shared": "*",
    "apollo-server-express": "^2.19.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "pino": "^6.8.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "tsyringe": "^4.4.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/pino": "^6.3.4",
    "@types/source-map-support": "^0.5.3",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "jest": "^26.6.3",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.2"
  }
}
